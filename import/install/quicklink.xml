<sailpoint>

	<ImportAction name="merge">
		<Configuration name="SystemConfiguration">
			<Attributes>
				<Map>
					<entry key='quickLinkCategories'>
						<value>
							<List>
								<QuickLinkCategory name="ProofpointPlugin"
									messageKey="Proofpoint Plugin" enabled="true"
									icon="quickLinkCategoryCustom" />
							</List>
						</value>
					</entry>
				</Map>
			</Attributes>
		</Configuration>
	</ImportAction>

	<QuickLink action="external" category="ProofpointPlugin"
		messageKey="Proofpoint Plugin" name="ProofpointPlugin" ordering="4">
		<Attributes>
			<Map>
				<entry key="url"
					value="plugins/pluginPage.jsf?pn=ProofpointPlugin" />
			</Map>
		</Attributes>
		<QuickLinkOptions allowSelf="true">
			<DynamicScopeRef>
				<Reference class="sailpoint.object.DynamicScope"
					name="Everyone" />
			</DynamicScopeRef>
		</QuickLinkOptions>
	</QuickLink>

</sailpoint>